#include <stdio.h>
int array(int arr[])
{
    int arr_1[6];
    int n=6;
    for(int i=0; i<=n ;i++)
   { 
    arr_1[i]=arr[6-i];
    printf("Your %d element is:%d\n",i,arr_1[i]);
   }
    return(1);
}
int main()
{
   int arr[] ={1,2,3,4,5,6,67};
   printf("Your reversal is%d",array(arr));
     return(0);   
}
