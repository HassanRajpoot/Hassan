#include<stdio.h>
#include<string.h>
int main()
{
    char str1[30],str2[30];
    int len,i,j =1,k=0,len1;
    char string1[60];
    printf("Enter your string:");
    gets(str1);
    len = strlen(str1);
    printf("Enter your string:");
    gets(str2);
    for(i=0;i<len;i++)
    {
        string1[k]=str1[i];
        string1[j]=str2[i];
        j = j+2;
        k = k+2;
    }
    printf("%s",string1);
}
